<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eeffa.app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_green_tree"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.eeffa.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.eeffa.app.VideoActivity"
            android:label="@string/title_activity_video"
            android:parentActivityName="com.eeffa.app.VideoListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eeffa.app.VideoListActivitys" />
        </activity>
        <activity
            android:name="com.eeffa.app.news_list"
            android:label="@string/title_activity_news__list"
            android:parentActivityName="com.eeffa.app.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eeffa.app.MainActivity" />
        </activity>
        <activity
            android:name="com.eeffa.app.NewsListActivity"
            android:label="@string/title_activity_news_list"
            android:parentActivityName="com.eeffa.app.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eeffa.app.MainActivity" />
        </activity>
        <activity
            android:name="com.eeffa.app.VideoListActivity"
            android:label="@string/title_activity_vertical_list"
            android:parentActivityName="com.eeffa.app.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eeffa.app.MainActivity" />
        </activity>
        <activity
            android:name="com.eeffa.app.EventListActivity"
            android:label="@string/title_activity_event_list"
            android:parentActivityName="com.eeffa.app.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eeffa.app.MainActivity" />
        </activity>
        <activity
            android:name="com.eeffa.app.EventActivity"
            android:label="@string/title_activity_event"
            android:parentActivityName="com.eeffa.app.EventListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eeffa.app.EventListActivity" />
        </activity>
        <activity
            android:name="com.eeffa.app.NwesActivity"
            android:label="@string/title_activity_nwes"
            android:parentActivityName="com.eeffa.app.NewsListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eeffa.app.NewsListActivity" />
        </activity>
        <activity
            android:name="com.eeffa.app.InfoActivity"
            android:label="@string/title_activity_info" 
            android:parentActivityName="com.eeffa.app.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eeffa.app.MainActivity" />
        </activity>
    </application>

</manifest>
